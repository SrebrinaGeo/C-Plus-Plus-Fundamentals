#include <iostream>
#include<string>
using namespace std;

int main()
{
    string expression;
    getline(cin, expression);

    int balance = 0;
    for (int i = 0; i < expression.length(); i++)
    {
        if (expression[i] == '(') balance++;
        else if (expression[i] == ')')
        {
            balance--;
            if (balance < 0) break;
        }
    }

    if (balance == 0) cout << "correct" << endl;
    else cout << "incorrect" << endl;
  
  	return 0;
}
