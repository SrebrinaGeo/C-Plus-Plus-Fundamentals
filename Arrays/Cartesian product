#include <iostream>
using namespace std;

void readArray(int array[], const int& n)
{
    for (int i = 0; i < n; i++)
    {
        cin >> array[i];
    }
}

int main()
{
    constexpr int MAX_N = 100, RANGE = 10;
    int array[MAX_N], counters[RANGE]{};

    int n;
    cin >> n;

    readArray(array, n);


    for (int i = 0; i < n; i++)
    {
        for (int j = 0; j < n; j++)
        {
            cout << array[i] * array[j] << " ";
        }
    }
    cout << endl;
    
    return 0;
}
