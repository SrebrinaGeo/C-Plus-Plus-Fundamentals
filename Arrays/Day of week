Arrays#include <iostream>
using namespace std;


int main()
{
    string daysOfTheWeek[] =
    {
        "Monday",
        "Tuesday",
        "Wednesday",
        "Thursday",
        "Friday",
        "Saturday",
        "Sunday"
    };

    int day;
    cin >> day;

    if (day < 1 || day >7)
    {
        cout << "Invalid day!" << endl;
    }

    else
    {
        cout << daysOfTheWeek[day - 1] << endl;
    }

    return 0;
}
