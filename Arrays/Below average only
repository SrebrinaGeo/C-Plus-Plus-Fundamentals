#include <iostream>
using namespace std;

int main()
{
    int n;
    cin >> n;

    int sum = 0;
    int product = 1;
    int numbers[n];
    int newArray[n];

    for (int i = 0; i < n; i++)
    {
        cin >> numbers[i];
        sum += numbers[i];
    }

    int average = sum / n;
    int newIndex = 0;

    for (int i = 0; i < n; i++)
    {
        if (numbers[i] <= average)
        {
            newArray[newIndex++] = numbers[i];
        }
    }

    int evenSum = 0;
    int oddSum = 0;
    for (int i = 0; i < newIndex; i++)
    {
        if (i % 2 == 0)
        {
            evenSum += newArray[i];
        }
        else
        {
            oddSum += newArray[i];
        }
    }

    int result = evenSum * oddSum;

    cout << result << endl;

    return 0;
}
