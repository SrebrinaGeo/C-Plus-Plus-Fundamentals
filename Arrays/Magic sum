#include <iostream>
using namespace std;

void readArray(int arr[], const int& n) 
{
    for (int i = 0; i < n; i++) cin >> arr[i];
}

int main()
{
    constexpr int MAX_N = 100;
    int arr[MAX_N];

    int n;
    cin >> n;

    readArray(arr, n);

    int sum;
    cin >> sum;

    for (int i = 0; i < n - 1; i++) 
    {
        for (int j = i + 1; j < n; j++) 
        {
            if (arr[i] + arr[j] == sum) cout << arr[i] << ' ' << arr[j] << endl;
        }
    }
  return 0;
}
