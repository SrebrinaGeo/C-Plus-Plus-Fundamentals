#include <iostream>
using namespace std;


int main()
{
    const int MAX_SIZE = 99;

    int numbers[MAX_SIZE];
    int evenNumbersSum = 0;

    int size;
    cin >> size;

    for (int i = 0; i < size; i++)
    {
        cin >> numbers[i];
    }

    for (int i = 0; i < size; i++)
    {
        if (numbers[i] % 2 == 0)
        {
            evenNumbersSum += numbers[i];
        }
    }

    cout << evenNumbersSum;

    return 0;
}
