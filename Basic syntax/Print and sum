#include <iostream>
#include <cmath>
#include <climits>
#include <math.h>
using namespace std;

int main()
{
    int start, end;
    cin >> start >> end;
    int sum = 0;

    for (int i = start; i <= end; i++)
    {
        sum += i;
        cout << i << " ";
    }

    cout << "\nSum: " << sum << endl;

    return 0;
}
