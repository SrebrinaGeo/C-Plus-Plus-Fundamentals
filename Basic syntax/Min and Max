#include <iostream>
#include<cmath>
#include<climits>
#include<math.h>
using namespace std;

int main()
{
    int a;
    cin >> a;
    int min = INT_MAX, max = INT_MIN;

    for (int i = 1; i <= a; i++)
    {
        int num;
        cin >> num;
        if (num > max) max = num;
        if (num < min) min = num;
    }

    cout << min << " " << max << endl;

    return 0;
}
